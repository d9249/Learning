using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.UI;

public class KG_LightControl : MonoBehaviour {


    public Slider LightSlider = null;

    public Light LightIntencity;

    public void LightSliderChange()
    {
        LightIntencity.intensity = LightSlider.value;
    }
}
